<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.notused.EHomeActivity" android:orientation="vertical">

    <LinearLayout style="@style/default_linearlayout">

        <TextView
                style="@style/home_tv"
                android:text="Material Type"
        />
        <android.support.v7.widget.AppCompatSpinner
                android:id="@+id/ehome_material_spinner"
                style="@style/home_spinner"
        />

        <TextView
                style="@style/home_tv"
                android:text="Location"
        />
        <android.support.v7.widget.AppCompatSpinner
                android:id="@+id/ehome_location_spinner"
                style="@style/home_spinner"
        />

        <TextView
                android:visibility="gone"
                style="@style/signin_btn"
                android:layout_marginTop="@dimen/_10sdp"
                android:text="@string/login"
        />
        <Button
                android:id="@+id/ehome_next"
                style="@style/next_button"
                android:layout_marginTop="@dimen/_50sdp"
                android:text="@string/continuetext"
        />

    </LinearLayout>

</LinearLayout>