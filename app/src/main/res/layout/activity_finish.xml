<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.notused.FinishActivity">

    <LinearLayout
            android:gravity="center"
            style="@style/default_linearlayout">

        <TextView
                android:gravity="center"
                android:text="@string/hour_meter_finish"
                style="@style/home_tv"
        />
        <EditText
                android:gravity="center"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp"
                android:textColor="@color/black"
                style="@style/et_background"
                android:inputType="number"
                android:layout_marginTop="@dimen/_1sdp"
                android:hint="@string/hour_meter_finish"
        />


        <Button
                android:id="@+id/finish_next"
                style="@style/next_button"
                android:layout_marginTop="@dimen/_50sdp"
                android:text="@string/logout"
        />

    </LinearLayout>
</LinearLayout>